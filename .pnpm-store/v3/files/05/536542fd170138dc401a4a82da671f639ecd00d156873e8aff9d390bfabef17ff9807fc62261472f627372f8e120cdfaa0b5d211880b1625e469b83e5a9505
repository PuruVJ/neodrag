import*as t from"fs";var o={path:"./dist/",logger:2,pipeline:{wrote:async e=>e.buffer,read:async e=>await t.promises.readFile(e.inputPath,"utf-8"),passed:async()=>!0,failed:async e=>`Error: Cannot process file ${e.inputPath}!`,accomplished:async e=>`Processed ${e.inputPath} in ${e.outputPath}.`,fulfilled:async e=>e.files>0?`Successfully processed a total of ${e.files} ${e.files===1?"file":"files"}.`:!1,changed:async e=>e}};export{o as default};
