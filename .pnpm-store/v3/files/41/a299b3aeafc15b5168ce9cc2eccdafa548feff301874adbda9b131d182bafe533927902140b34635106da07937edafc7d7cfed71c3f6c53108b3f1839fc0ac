/**
 * Asynchronously run code.
 *
 * @param {{toString(): string}} file
 *   JS document to run.
 * @param {unknown} options
 * @return {Promise<*>}
 */
export function run(file: {
    toString(): string;
}, options: unknown): Promise<any>;
/**
 * Synchronously run code.
 *
 * @param {{toString(): string}} file
 *   JS document to run.
 * @param {unknown} options
 * @return {*}
 */
export function runSync(file: {
    toString(): string;
}, options: unknown): any;
