/**
 * @param {Link} node
 * @param {Parent|undefined} _
 * @param {Context} context
 * @param {SafeOptions} safeOptions
 * @returns {string}
 */
export function link(
  node: Link,
  _: Parent | undefined,
  context: Context,
  safeOptions: SafeOptions
): string
export namespace link {
  export {linkPeek as peek}
}
export type Link = import('mdast').Link
export type Root = import('mdast').Root
export type Content = import('mdast').Content
export type Node = Root | Content
export type Parent = Extract<Node, import('mdast').Parent>
export type Context = import('../types.js').Context
export type SafeOptions = import('../types.js').SafeOptions
export type Exit = import('../types.js').Exit
/**
 * @param {Link} node
 * @param {Parent|undefined} _
 * @param {Context} context
 * @returns {string}
 */
declare function linkPeek(
  node: Link,
  _: Parent | undefined,
  context: Context
): string
export {}
