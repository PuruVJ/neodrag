/**
 * Create an extension to support frontmatter (YAML, TOML, and more).
 *
 * @param {Options} [options='yaml'] One preset or matter, or an array of them.
 * @returns {HtmlExtension}
 */
export function frontmatterHtml(
  options?: import('../matters.js').Options | undefined
): HtmlExtension
export type HtmlExtension = import('micromark-util-types').HtmlExtension
export type Handle = import('micromark-util-types').Handle
export type Options = import('../matters.js').Options
