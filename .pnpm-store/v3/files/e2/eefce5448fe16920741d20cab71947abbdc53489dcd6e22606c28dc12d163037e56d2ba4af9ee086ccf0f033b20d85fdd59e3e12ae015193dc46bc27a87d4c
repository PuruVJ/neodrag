/** @param {any} thing */
export function is_primitive(thing: any): boolean;
/** @param {any} thing */
export function is_plain_object(thing: any): boolean;
/** @param {any} thing */
export function get_type(thing: any): any;
/** @param {string} str */
export function stringify_string(str: string): string;
/** @type {Record<string, string>} */
export const escaped: Record<string, string>;
export class DevalueError extends Error {
    /**
     * @param {string} message
     * @param {string[]} keys
     */
    constructor(message: string, keys: string[]);
    path: string;
}
