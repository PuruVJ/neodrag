export declare type Event = 'change' | 'leave' | 'enter' | 'init';
export declare type Options = {
    root?: HTMLElement | null;
    rootMargin?: string;
    threshold?: number | number[];
    unobserveOnEnter?: boolean;
};
export declare type Position = {
    x?: number;
    y?: number;
};
declare type Direction = 'up' | 'down' | 'left' | 'right';
export declare type ScrollDirection = {
    vertical?: Direction;
    horizontal?: Direction;
};
export declare type ObserverEventDetails = {
    inView: boolean;
    entry: IntersectionObserverEntry;
    scrollDirection: ScrollDirection;
    node: HTMLElement;
    observer: IntersectionObserver;
};
export declare type LifecycleEventDetails = {
    node: HTMLElement;
    observer: IntersectionObserver;
};
export {};
