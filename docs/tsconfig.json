{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"$layouts/*": ["src/layouts/*"],
			"$components/*": ["src/components/*"],
			"$stores/*": ["src/stores/*"],
			"$actions/*": ["src/actions/*"],
			"$helpers/*": ["src/helpers/*"]
		}
	},
	"include": ["./markdown/*", "./src/auto-imports.d.ts", ",./src/**/*.{ts,js}"]
}
